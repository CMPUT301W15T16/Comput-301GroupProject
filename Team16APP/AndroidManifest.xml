<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.team16app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ApproverClaimViewActivity"
            android:label="@string/title_activity_approver_claim_view" >
        </activity>
        <activity
            android:name=".AddItemActivity"
            android:label="@string/title_activity_add_item" >
        </activity>
        <activity
            android:name=".AddClaimActivity"
            android:label="@string/title_activity_add_claim" >
        </activity>
        <activity
            android:name=".TakePhotoActivity"
            android:label="@string/title_activity_take_photo" >
        </activity>
        <activity
            android:name=".ApproverClaimListActivity"
            android:label="@string/title_activity_approver_claim_list" >
        </activity>
        <activity
            android:name=".ClaimantItemListActivity"
            android:label="@string/title_activity_claimant_item_list" >
        </activity>
    </application>

</manifest>
